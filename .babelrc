{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  },
  "plugins": [
    ["@babel/proposal-decorators", { "legacy": false }],
    ["@babel/proposal-class-properties", { "loose": false }],
    ["@babel/transform-decorators"]
  ]
}
